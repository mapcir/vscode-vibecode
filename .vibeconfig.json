{
    "proxy": null,
    "defaultProvider": "gemini",
    "providers": {
        "gemini": {
            "baseURL": "https://generativelanguage.googleapis.com/v1beta",
            "method": "POST",
            "headers": {
                "Content-Type": "application/json"
            },
            "defaultModel": "gemini-1.5-flash",
            "models": {
                "gemini-1.5-pro": {
                    "contextWindow": 2097152,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning",
                        "long-context"
                    ]
                },
                "gemini-1.5-flash": {
                    "contextWindow": 1048576,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "fast"
                    ]
                }
            }
        },
        "openai": {
            "baseURL": "https://api.openai.com/v1",
            "method": "POST",
            "headers": {
                "Authorization": "Bearer ${OPENAI_API_KEY}",
                "Content-Type": "application/json"
            },
            "defaultModel": "gpt-4o",
            "models": {
                "gpt-4o": {
                    "contextWindow": 128000,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning",
                        "agent",
                        "top-tier"
                    ]
                },
                "gpt-4-turbo": {
                    "contextWindow": 128000,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning"
                    ]
                },
                "gpt-3.5-turbo": {
                    "contextWindow": 16000,
                    "supportsImages": false,
                    "tags": [
                        "fast",
                        "cheap"
                    ]
                }
            }
        },
        "claude": {
            "baseURL": "https://api.anthropic.com/v1",
            "method": "POST",
            "headers": {
                "x-api-key": "${ANTHROPIC_API_KEY}",
                "anthropic-version": "2023-06-01",
                "Content-Type": "application/json"
            },
            "defaultModel": "claude-3-5-sonnet-20240620",
            "models": {
                "claude-3-5-sonnet-20240620": {
                    "contextWindow": 200000,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning",
                        "top-tier"
                    ]
                },
                "claude-3-opus-20240229": {
                    "contextWindow": 200000,
                    "supportsImages": true,
                    "tags": [
                        "code",
                        "reasoning",
                        "architecture"
                    ]
                },
                "claude-3-haiku-20240307": {
                    "contextWindow": 200000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "fast",
                        "light"
                    ]
                }
            }
        },
        "deepseek": {
            "baseURL": "https://api.deepseek.com/v1",
            "method": "POST",
            "headers": {
                "Authorization": "Bearer ${DEEPSEEK_API_KEY}",
                "Content-Type": "application/json"
            },
            "defaultModel": "deepseek-chat",
            "models": {
                "deepseek-coder": {
                    "contextWindow": 64000,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "reasoning"
                    ]
                },
                "deepseek-chat": {
                    "contextWindow": 64000,
                    "supportsImages": false,
                    "tags": [
                        "general"
                    ]
                }
            }
        },
        "ollama": {
            "baseURL": "http://localhost:11434/v1",
            "type": "local",
            "method": "POST",
            "headers": {
                "Content-Type": "application/json"
            },
            "defaultModel": "llama3",
            "models": {
                "llama3": {
                    "contextWindow": 8192,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "local"
                    ]
                },
                "codellama": {
                    "contextWindow": 16384,
                    "supportsImages": false,
                    "tags": [
                        "code",
                        "local"
                    ]
                }
            }
        }
    },
    "defaultTerminalPolicy": "turbo",
    "terminalPolicy": {
        "off": {
            "allowedCommands": []
        },
        "safe": {
            "blockedCommands": [
                "rm -rf",
                "sudo",
                "dd"
            ]
        },
        "turbo": {
            "blockedCommands": [
                "rm -rf /",
                "sudo rm",
                "dd"
            ],
            "requireConfirmation": [
                "npm publish"
            ]
        },
        "custom": {
            "allowedCommands": [
                "npm",
                "ls",
                "cat",
                "grep"
            ],
            "blockedCommands": [
                "rm -rf",
                "sudo"
            ],
            "requireConfirmation": [
                "npm install"
            ]
        }
    },
    "defaultAuthorityMode": "Autonomous",
    "authorityModes": {
        "autonomous": {
            "description": "Full Agent - Do everything until done",
            "maxIterations": 10,
            "autoTest": true,
            "stopOnlyFor": [
                "high_risk_actions"
            ]
        },
        "collaborative": {
            "description": "Always ask before executing",
            "requireApprovalFor": [
                "write_file",
                "run_command"
            ]
        }
    },
    "rag": {
        "enabled": true,
        "autoIndex": true,
        "maxFiles": 500
    },
    "autoClearHistory": {
        "enabled": true,
        "mode": "smart",
        "keepSystemPrompt": true
    }
}
