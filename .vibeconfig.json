    "defaultTerminalPolicy": "turbo",
    "terminalPolicy": {
        "off": {
            "allowedCommands": []
        },
        "safe": {
            "blockedCommands": [
                "rm -rf",
                "sudo",
                "dd"
            ]
        },
        "turbo": {
            "blockedCommands": [
                "rm -rf /",
                "sudo rm",
                "dd"
            ],
            "requireConfirmation": [
                "npm publish"
            ]
        },
        "custom": {
            "allowedCommands": [
                "npm",
                "ls",
                "cat",
                "grep"
            ],
            "blockedCommands": [
                "rm -rf",
                "sudo"
            ],
            "requireConfirmation": [
                "npm install"
            ]
        }
    },
    "defaultAuthorityMode": "Autonomous",
    "authorityModes": {
        "autonomous": {
            "description": "Full Agent - Do everything until done",
            "maxIterations": 10,
            "autoTest": true,
            "stopOnlyFor": [
                "high_risk_actions"
            ]
        },
        "collaborative": {
            "description": "Always ask before executing",
            "requireApprovalFor": [
                "write_file",
                "run_command"
            ]
        }
    },
    "rag": {
        "enabled": true,
        "autoIndex": true,
        "indexOnStartup": true,
        "maxFiles": 500,
        "topK": 3,
        "maxLineLength": 1000
    },
    "autoClearHistory": {
        "enabled": true,
        "mode": "smart",
        "keepSystemPrompt": true,
        "keepRAGContext": false,
        "showNotification": true
    }
}
